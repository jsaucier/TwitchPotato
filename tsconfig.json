        "./Source/App/js/Lib/jquery.d.ts",
        "./Source/App/js/Player/Controller.ts",
        "./Source/App/js/Player/Player.ts",
        "./Source/App/js/Player/Players.ts",
        "./Source/App/js/Player/Selector.ts",
        "./Source/App/js/Storage.ts",
        "./Source/App/js/Twitch.ts",
        "./Source/App/js/chat.ts",
        "./Source/App/js/input.ts",
        "./Source/App/js/notification.ts",
        "./Source/App/js/potato.ts",
        "./Source/App/js/utils.ts"
