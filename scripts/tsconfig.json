{
    "version": "1.4.1",
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outDir": "../js/",
        "sourceMap": true
    },
    "filesGlob": [
        "./*.ts",
        "./*/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./_potato.d.ts",
        "./chat.ts",
        "./chrome.d.ts",
        "./inject.ts",
        "./input.ts",
        "./jquery.d.ts",
        "./notification.ts",
        "./player.ts",
        "./potato.ts",
        "./storage.ts",
        "./twitch.ts",
        "./utils.ts",
        "./Guide/Content.ts",
        "./Guide/ContextMenu.ts",
        "./Guide/FollowMenu.ts",
        "./Guide/Guide.ts",
        "./Guide/MenuItem.ts",
        "./Guide/Timer.ts"
    ]
}
